import java.util.ArrayList;

public class Main {
    public static void main(String[] args) {
        ArrayList<String> strings = new ArrayList<>();
        String[] arr = {"a", "b", "c", "d", "e"};
        for (int i = 0; i < arr.length; i++) {
            strings.add(arr[i]);
        }
        System.out.println(strings);
    }
}
